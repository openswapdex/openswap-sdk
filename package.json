      "@ijstech/eth-wallet": "0.1.26",
      "@openzeppelin/contracts": "4.8.0",
      "@scom/contract-deployer": "^0.1.6",
      "@types/mocha": "9.1.1",
      "@types/node": "18.8.4",
      "ganache": "7.5.0",
      "mocha": "10.0.0",
      "ts-node": "10.9.1",
      "typescript": "4.8.4"
  }
