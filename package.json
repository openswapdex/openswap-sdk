{
  "name": "@openswap/sdk",
  "version": "0.1.11",
  "description": "",
  "typings": "types/index.d.ts",
  "main": "lib",
  "scripts": {
    "build": "rm -rf lib && rm -rf dist && rm -rf types && npm run build:contracts && npm run updateHash && npm run build:contracts && npm run build:lib && npm run build:dist && npm run build:deployer",
    "updateHash": "ts-node tools/updateHash.ts",
    "build:contracts": "rm -rf src/contracts && sol2ts solidity.config.json",
    "build:test": "sol2ts solidity-test.config.json",
    "build:deployer": "rm -rf deployer && npx @ijstech/cli bundle deployer",
    "build:dist": "npx @ijstech/compiler",
    "build:lib": "tsc",
    "deployer": "npx @ijstech/cli serve 8080 deployer",
    "test": "mocha --exit --require ts-node/register --timeout 100000 test/*.test.ts",
    "test:hybridRouter": "mocha --ui tdd --exit --require ts-node/register --timeout 100000 test/hybridRouter.test.ts",
    "build:plugin": "compile 0.6.11 node_modules/@openswap/openswap-core/contracts plugin/contracts",
    "deploy": "ts-node tools/deploy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ijstech/eth-contract": "0.1.18"
  },
  "devDependencies": {
    "@ijstech/cli": "^0.1.32",
    "@ijstech/compiler": "^0.1.15",
    "@ijstech/eth-wallet": "^0.1.51",
    "@openzeppelin/contracts": "4.8.2",
    "@scom/contract-deployer": "^0.1.6",
    "@scom/dapp": "^0.1.6",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.1",
    "@types/node": "18.8.4",
    "chai": "4.3.7",
    "chai-bignumber": "3.1.0",
    "ganache": "7.5.0",
    "mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "web3-providers-http": "1.9.0"
  }
}
